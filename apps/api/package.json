  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=dev MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" node -r tsconfig-paths/register dist/apps/api/src/main",
    "dev": "cross-env NODE_ENV=dev MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" nest start --watch --preserveWatchOutput",
    "debug": "cross-env NODE_ENV=test MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" nest start --debug --watch",
    "start:test": "cross-env NODE_ENV=test MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" node -r tsconfig-paths/register dist/apps/api/src/main",
    "prod:cn": "cross-env NODE_ENV=prod MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" node -r tsconfig-paths/register dist/apps/api/src/main",
    "prod:us": "cross-env NODE_ENV=produs MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" node -r tsconfig-paths/register dist/apps/api/src/main",
    "prod:ap": "cross-env NODE_ENV=prodap MICRO_SERVER_NAME=api PROJECT_ROOT=\"$(pwd)\" node -r tsconfig-paths/register dist/apps/api/src/main",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint \"{src,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "clean": "rimraf dist",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push --skip-generate",
    "db:generate": "prisma generate && node scripts/generate-db-crud.js",
    "db:format": "prisma format",
    "db:seed": "ts-node prisma/seed.ts",
    "update:model-pricing": "ts-node scripts/update-model-pricing.ts",
    "validate:versions": "ts-node scripts/validate-api-versions.ts"
  },
